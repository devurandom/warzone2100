PATH="${PREFIX}/bin:${PATH}"

CFLAGS="-pipe -m32 -march=i686 -O2 -g"
CXXFLAGS="${CFLAGS}"
CPPFLAGS="-I${PREFIX}/include -I${PREFIX}/include/SDL"
LDFLAGS="-L${PREFIX}/lib"

